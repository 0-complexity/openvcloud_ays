basepath                       = '/opt/OpenvStorage/webapps/frontend'

git.export.1                   =
    dest:'/opt/OpenvStorage/webapps/frontend/lib/ovs/images/',
    link:'False',
    merge:'True',
    source:'images/*',
    url:'https://docs.greenitglobe.com/binary/openvstorage_branding',

git.export.2                   =
    dest:'/opt/OpenvStorage/webapps/frontend/css/',
    link:'False',
    overwrite:'True',
    source:'css/*',
    url:'https://docs.greenitglobe.com/binary/openvstorage_branding',

git.export.3                   =
    dest:'/opt/OpenvStorage/webapps/frontend/app/views/site/',
    link:'False',
    overwrite:'True',
    source:'app/views/site/login.html',
    url:'https://docs.greenitglobe.com/binary/openvstorage_branding',

git.export.4                   =
    dest:'/opt/OpenvStorage/webapps/frontend/app/views/',
    link:'False',
    merge:'True',
    source:'app/views/index.html',
    url:'https://docs.greenitglobe.com/binary/openvstorage_branding',

platform.supported             = 'generic'

url                            = 'https://docs.greenitglobe.com/binary/openvstorage_branding'
