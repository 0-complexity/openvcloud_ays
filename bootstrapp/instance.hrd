listen.port           =@ASK descr:'listent port of the app' default:5000 type:'int'
ovc_git               =@ASK descr:'path to the OVC_GIT repo' default:'/opt/OVC_GIT' type:'str'
reflector.ip.priv     =@ASK descr:'private ip of the reflector VM' type:'str'
reflector.ip.pub      =@ASK descr:'public ip of the reflector VM (ip of the cloudscpace)' type:'str'
reflector.port        =@ASK descr:'ssh connection port to reflector' type:'int'
reflector.name        =@ASK descr:'service name of the master node' default:'jumpscale__node.ssh__ovc_reflector' type:'str'
reflector.user        =@ASK descr:'safe user on the reflector' default:'guest' type:'str'
